{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },

  "extends": ["plugin:react/recommended", "next", "next/core-web-vitals"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "plugins": ["react"],
  "globals": {
    "NodeJS": true
  },
  "rules": {
    "@typescript-eslint/explicit-function-return-type": "off"
  }
}
